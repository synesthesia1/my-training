first = int(input("Введите первые 3 числа: "))
second = int(input("Введите вторые 3 числа: "))
third = int(input("Введите последние 3 числа: "))
if first == second == third:
    print(3)
elif first == second or second == third or first == third:
    then: print(2)
elif first != second != third:
    print(0)
